#!/bin/sh
#
# KakuteF7 specific board defaults
#------------------------------------------------------------------------------

if param compare SYS_AUTOSTART 0
then
	# Select the Generic 250 Racer by default
	param set SYS_AUTOSTART 4050
	set AUTOCNF yes
fi

param import-as-default /etc/init.d/board_params

set LOGGER_BUF 30
